[package]
name = "{{packageName}}"
version = "{{appVersion}}"
description = "{{appDescription}}"
homepage = "{{homepage}}"
repository = "{{repoUrl}}"
keywords = {{json_str keywords}}
categories = {{json_str categories}}
authors = ["{{{author}}}"]
readme = "README.md"
license = "{{license}}"
edition = "2018"

[package.metadata.docs.rs]
all-features = true

[package.metadata.playground]
features = ["all"]

[features]
default = []
all = []

[dependencies]
log = "0.4"